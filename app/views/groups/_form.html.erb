<%= render :partial => "shared/generic_errors", :locals => { :object => @group } %>
<%= form_for(@group) do |form| %>     
<p>
	<%= form.label :name %><br>
	<%= form.text_field :name %>
</p>
<p>
	<%= form.label :description %><br>
	<%= form.text_field :description %>
</p>
<p>
	<%= form.label :tags %><br>
	<%= form.text_field :tags %>
</p>
<p><%= form.submit %></p>
<% end %>